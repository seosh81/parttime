function _jsGetDetailParam(){
var returnParam = "";
if(daum.getParam("autocvt") && daum.getParam("autocvt")=="1"){
returnParam = returnParam + "&autocvt=1"
}
return returnParam + "&period="+tabParams.period+"&DocType="+tabParams.DocType+"&sd="+tabParams.sd+"&ed="+tabParams.ed+"&SA="+tabParams.SA+"&f="+tabParams.f;
}
SF.C.blogTab = {
clickCodeMap : [],
clickLog : function(element, docid, p, event) {
if (docid && docid != 0) {
var codeMap = SF.C.blogTab.clickCodeMap[docid];
var logStr = "";
for (key in codeMap) {
if (logStr != "") {
logStr += "&";
}
if (key == "p") {
logStr += "p=" + p;
} else {
logStr += key + "=" + codeMap[key];
}
}
smartLog(element, logStr, event);
}
},
toggleMap : function(el, event, docId) {
var parentEl = daum.$E('br_' + docId);
var handleElem = daum.$$('.spBlogTabMapToggle', parentEl)[0];
SF.C.blogTab.currentPlace = parentEl;
if (parentEl && handleElem) {
var mapElem = daum.$$('.map', parentEl)[0];
var docId = mapElem ? mapElem.getAttribute('doc_id') : 0;
if(parentEl.hasClassName("open")) {
parentEl.removeClassName("open");
handleElem.innerHTML = "지도보기";
daum.Element.removeClassName(el, 'btn_on');
if (docId) {
SF.C.blogTab.clickLog(el, docId, 21, event);
}
} else {
parentEl.addClassName("open");
handleElem.innerHTML = "지도접기";
daum.Element.addClassName(el, 'btn_on');
if(mapElem.innerHTML == "") {
var confirmId = mapElem.getAttribute('confirm_id') || 0;
daum.load("http://map.search.daum.net/mapsearch/srcid.daum?output=jsonp&id=" + confirmId + "&servicename=tot_blog&callback=SF.C.blogTab.mapFillPlaceInfo", null, {charset:"utf-8"});
}
if (docId) {
SF.C.blogTab.clickLog(el, docId, 22, event);
}
}
}
