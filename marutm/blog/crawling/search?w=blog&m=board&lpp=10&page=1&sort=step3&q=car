/**
* Tico를 사용하기 위해 모듈을 시작합니다.
* @author utsman
*/

Tico.getInstance().start({
"SERVICE_CODE": "tot_search_ft"
});

Tico.getInstance().setParams({
"login_yn": login_yn,
"uk": uk,
"sugLocCode": sugLocCode
});
window.onerror = function(errorMsg, url, linenum){
var event = window.event || {};
var stack = '[' + errorMsg + '] [' + url + '] [' + linenum + ']';
var ex = {message: event.errorMessage||errorMsg, stack: stack};
Tico.getInstance().send(ex, 'GLOBAL_ERROR', {
TYPE: Tico.TYPE.GLOBAL_ERROR
});
return false;
};
/**
* cloudweb change <br>
* cloudweb 동작시 재 변경
* @author lion
* @function
* @name onload addevent
*/
daumSF.Event.addEvent(window, "load", function() {
window.setTimeout( function() {
try {
var spon = $("sponsorLinkColl");
var prem = $("premiumLinkColl");
var splink = $("splinkColl");
var widelink = $("wideLinkColl");
var flag = false;
var cwLog = "";
if(spon) {
if(spon.style.display == "none") {
spon.style.display = "";
cwLog = "sponsorLinkColl";
}
}
if(prem) {
if (prem.style.display == "none") {
prem.style.display = "";
flag = true;
if(cwLog == "") {
cwLog = "premiumLinkColl";
} else {
cwLog = cwLog + "_premiumLinkColl";
}
}
}
if(splink) {
if (splink.style.display == "none") {
splink.style.display = "";
flag = true;
if(cwLog == "") {
cwLog = "splinkColl";
} else {
cwLog = cwLog + "_splinkColl";
}
}
}
if(widelink) {
if (widelink.style.display == "none") {
widelink.style.display = "";
flag = true;
if(cwLog == "") {
cwLog = "wideLinkColl";
} else {
cwLog = cwLog + "_wideLinkColl";
}
}
}
if(flag) {
var line = $$("div .line");
for(var k=0,cnt=line.length;k<cnt;k++) {
if (line[k].style.display == "none") {
line[k].style.display = "";
}
}
}
if(cwLog != "") {
SF.M.jsCallScript("cwLogCallScript", "http://sl.search.daum.net/free_log/FreeLogServlet?from=cloudweb&log=" + encodeURIComponent("&cw=" + cwLog + "&uk="+uk+"&mk="+mk+"&q="+EnQuery), {type : "text/javascript",charset : "euc-kr",defer : false});
}
} catch(e) {}
}
,200);
});
/**
* 이미지에러 처리함수<br>
* 이미지 에러시 img 태그 변경 및 파라미터 정리
* @author hjkim83 yshwang
* @function
* @name SF.onImgErr
* @param options {isHide, wrapper, hasBorder}
* @param el 에러가 발생한 object(엘리먼트의 this)
*/
SF.onImgErr = function(options, el){
var obj = el ? el : daum.Event.getElement(window.event);
var opt = {
isHide: false,
wrapper: null,
hasBorder: true
};
daum.extend(opt,options || {});
if(opt.wrapper == null){
opt.wrapper = obj.parentNode.parentNode;
}
if(opt.isHide){
opt.wrapper.style.display="none";
}else{
obj.src = 'http://i1.daumcdn.net/imgsrc.search/search_all/2012/image/blank_img.gif';
obj.className = "noImage";
if(opt.hasBorder){
obj.style.border= "1px solid #ececec";
}
}
};
/*
yshwang. chrome issue fix.
img의 src가 비어있을경우 request없이 바로 oneeror수행되면서 함수가 끝나지 않았는데 innerHTML로 함수가 실행된곳이 없어져버림.
setTimeout으로 늦게 수행되도록 수정.
*/
SF._jsImgErr = function(sname, idx, _options, midfix) {
try{
var mfix = (midfix)?midfix:"_img_";
var idName = sname + mfix + idx;
var obj = daum.$(idName);
var durl="", str="";
var options = {
q : "", sv : "", ex : "",
opt : "N", x : "", 	y : "",
pClass : "noImgAr",
pStyle : ""
};
daum.extend(options, _options || {});
if (options.q != "") {
/*durl = '<img src="http://group3.magpie.daum.net/magpie/opencounter/Open.do?service=search_imgfail&key='+ options.q +'&extra='+ options.ex +'&host='+ options.sv +'" width="0" height="0" alt="" />';*/
}
if (options.opt == "Y") {
str = '<span class="'+ options.pClass +'" style="width:'+ options.x +'px;height:'+ options.y +'px;'+ options.pStyle +'">&nbsp;'+ durl +'</span>';
setTimeout(function(){
obj.innerHTML = str;
}, 0);
} else {
setTimeout(function(){
obj.innerHTML = durl;
}, 0);
obj.style.display = "none";
}
}catch(e){
}
};
/* 평점 모듈 참여자 카운트 콤마 추가 오버라이드, lion, 2013.02.28 */
_jsRatingModule.prototype.commentCallback = function(obj){
if( typeof obj != "undefined" ) {
var result = null;
var targetUrl = "";
var ratingCommentArea = daum.$(this.id["commentList"]);
var point_cnt = obj.point_cnt;
var point = new Number(obj.point);	/* PSN Count */
ratingCommentArea.innerHTML = "";
daum.$(this.id["pointNum"]).innerHTML = point.toFixed(1);
daum.$(this.id["pointPsnCnt"]).innerHTML = _jsInsertComma(point_cnt);
daum.$(this.id["pointImg"]).style.width = point.toFixed(1) * 10 +"%";
if( this.id["ratingType"] ) {
/* Only MovieScc */
this.rating_type = (obj.release_status == "미개봉") ? "before" : "after";
daum.$(this.id["ratingType"]).value = this.rating_type;
if( this.rating_type == "before") {
result = obj.detail.rating_before.content;
targetUrl = obj.detail.rating_before.link;
} else {
result = obj.detail.rating_after.content;
targetUrl = obj.detail.rating_after.link;
}
} else {
result = obj.detail.rating.content;
targetUrl = obj.detail.rating.link;
}
commentCnt = ( result.length >= 3 ) ? 3 : result.length;
if( commentCnt == 0 ) {
this.showFormLayer();
daum.$(this.id["commentWrapper"]).style.display = "none";
} else {
for (var i=0 ; i<commentCnt ; i++) {
var data = this.makeComment(i, commentCnt, result[i].content[1].content, result[i].content[2].content, new Number(result[i].content[0].content), targetUrl);
ratingCommentArea.innerHTML += data;
}
daum.$(this.id["commentWrapper"]).style.display = "block";
}
if(point_cnt > 3){
daum.$E(this.id["ratingMore"]).removeClassName("hide");
}
}
};
</script>
