S0: '저작자표시-비영리-동일조건 변경허락'
},
dataType: 'ccl'
};
so.options.push(cclConfig);
so.urlParams.ccl = {'ccl_derivative': '', 'ccl_commercial': ''};
so.relatedElems.push(daum.$('cclOption'));
domReady(function(){
var searchOptions = new SF.M.searchOption(so);
if ( tabParams.period == 'u') {
searchOptions.updateTermStr();
}
if ( !(tabParams.period == '' || tabParams.period == 'a') ) {
_jsTermOptionShowHide();
}
});
})();
