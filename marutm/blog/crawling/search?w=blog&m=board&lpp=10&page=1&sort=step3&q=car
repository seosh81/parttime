(function($ds) {
$ds.define.YellowClip = { "ON" : "expand", "OFF" : "off" };
$ds.template.RequestString = '?q=KEYWORD&mod=json&code=CHARSET&usertype='+sugLocCode;
$ds.template.RequestStringAnswer = '?q=KEYWORD&top=0&sq=FMKEYWORD';
var myKeyword = '<a href="http://p.search.daum.net/keyword?smenu=keyword" class="ico_comm2 my_keyword" target="_blank">My검색어</a>';
$ds.template.Footer = new rudolphUtil.Template( [
'<div class="fl">' + myKeyword + '#{moreWord}</div>',
'<div class="fr">',
'<a href="#" class="isSuggest">서제스트란?</a>',
'<span class="bar" style="display:#{offUsable}"> | </span>',
'<a href="#" class="offSuggest" style="display:#{offUsable}">기능끄기</a>',
'<span class="offGuide">기능을 다시 켤 때는 v 을 클릭하세요</span>',
'</div>'
].join('') );
$ds.template.RecentFooter = new rudolphUtil.Template( [
'<div class="fl">' + myKeyword + '</div>',
'<div class="fr">#{recentButton}</div>'
].join('') );
$ds.view.FooterView.prototype.initialize = function () {
var self = this;
rudolphUtil.addEvent( this.container, "mousedown", function(ev){
var el = rudolphUtil.getElement( ev );
if( rudolphUtil.hasClassName( el, "tailMore" ) ){
self.dispatchEvent( "onTailMore" );
} else if( rudolphUtil.hasClassName( el, "headMore" ) ){
self.dispatchEvent( "onHeadMore" );
} else if( rudolphUtil.hasClassName( el, "closeSuggest" ) ){
self.dispatchEvent( "onCloseSuggest" );
} else if( rudolphUtil.hasClassName( el, "isSuggest" ) ){  //서제스트란?
self.dispatchEvent( "onIsSuggest", { event : ev } );
smartLog(this, 's=TO&a=CLJJ&p=5&r=1', event);
} else if( rudolphUtil.hasClassName( el, "offSuggest" ) ){  //기능끄기
$ds.Service.block( true );
smartLog(this, 's=TO&a=CLJJ&p=6&r=1', event);
} else if( rudolphUtil.hasClassName( el, "deleteRecent" ) ){ //최근검색어 기록삭제
smartLog(this, 's=TO&a=CLJJ&p=2&r=1', event);
} else if( rudolphUtil.hasClassName( el, "toggleRecent off" ) ){ //최근검색어 숨기기
smartLog(this, 's=TO&a=CLJJ&p=3&r=1', event);
} else if( rudolphUtil.hasClassName( el, "toggleRecent on" ) ){ //최근검색어 보기
smartLog(this, 's=TO&a=CLJJ&p=4&r=1', event);
} else if( rudolphUtil.hasClassName( el, "ico_comm2 my_keyword" ) ){ //My검색어
smartLog(this, 's=TO&a=CLJJ&p=1&r=1', event);
}
rudolphUtil.stopEvent( ev );
} );
};
$ds.Service.init();
$ds.Instance.prototype.visible = function( yellowClipState, isNotEmptyKeyword ){
SF.M.detailSearch.close();
if( $ds.module.ActivationFlag.get() == true ){
this._instanceReady();
this.boxView.visible( yellowClipState, isNotEmptyKeyword );
}
};
window.suggestInstance = $ds.Service.add(
"q",
"wrap_suggestBox",

"http://sug.search.daum.net",

"/total_search_pssuggest",
"suggestInstance.dataModel.forceLoadComplete",
true,
"http://instant.search.daum.net",
"/suggest/index.daum",
"suggestInstance.dataModel.forceLoadCompleteAnswer"
)
.setEncodeKeyword( "utf_in_out" )
.setBeforeSubmitFunc(function() {
var f = document.search;
f.sug.value = "";
f.DA.value = "";
f.nil_suggest.value = "";
var sgtq = null, 
sn = null; 
var _ci = function() {
return document.createElement('input');
};
if (suggestInstance.answerView.isHighlight()) {
f.sug.value = "totex";
f.DA.value = "IHPV";
f.nil_suggest.value = "sugsch";
} else {
var item = suggestInstance.getSelectedItem();
if( item ){
sgtq = suggestInstance.keywordInputView.getDefaultKeyword();
sn = item.index + 1;
if (sgtq != null && sgtq != "" ) {

SF.M.jsCallScript("sgtqLogCallScript", "http://sl.search.daum.net/free_log/FreeLogServlet?from=sgtq&log=" + encodeURIComponent("d=P" + "&uq=" + sgtq + "&sq=" + f.q.value + "&o=" + sn), {type : "text/javascript",charset : "utf-8",defer : false});
}
if (item.isRecent) {
f.DA.value = "ARIV";
if (!f.nil_profile) {
var input = document.createElement('input');
input.type = 'hidden';
input.name = 'nil_profile';
f.appendChild(input);
} else {
f.nil_profile.value = '';
}
f.nil_profile.value = "reckwd";
} else {
f.DA.value = "IHPV";
f.nil_suggest.value = "sugsch";
}
} else {
f.nil_suggest.value = "btn";
f.DA.value = "SBCO";
}
}
check_submit();
})
.setYellowClip( "daumSuggest" )
.setLimit( [15,0] )
.setLimitMatch(false)
.setForm( document.search );
if (login_yn == 'Y') {
window.suggestInstance.setEnableRecentKeyword()
}
})(rudolph);
daum.suggest = rudolph;
<!-- end SA 삭제 -->
})();
</script>
