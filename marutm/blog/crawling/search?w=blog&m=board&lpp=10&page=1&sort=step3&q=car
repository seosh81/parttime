<script type="text/javascript">
(function(){
daum.Array.each(daum.$$("#mNav ul.gnb_comm li a.tab"), function (el, index) {
var check = el.href.toLowerCase().indexOf("/search?w=");
if(check==-1){
check = el.href.toLowerCase().indexOf("/bing?w=");
}
if(check==-1){
el.target = "_blank";
}else{
el.href = SF.changeUrlParam({
"spacing": (daum.getParam('spacing')!=null) ? daum.getParam('spacing') : ""
}, el.href);
}
});
})();
(function(){
var gnbToggleBtn = daum.$E("gnbToggleBtn");
if(gnbToggleBtn){
daum.Event.addEvent(gnbToggleBtn, 'click', daum.Function.bind(function(e) {
var deffenceLists = daum.$$("#daumGnb .deffence");
if(daum.Element.hasClassName(this, "btn_open")){
/* 닫혀있는 상태 */
daum.Element.removeClassName(this, "btn_open");
daum.Element.addClassName(this, "btn_close");
daum.Array.each(deffenceLists, function(el, idx){
el.style.display = "block";
});
}else{
/* 열려있는 상태 */
daum.Element.removeClassName(this, "btn_close");
daum.Element.addClassName(this, "btn_open");
daum.Array.each(deffenceLists, function(el, idx){
if(!daum.Element.hasClassName(el, "on")){
el.style.display = "none";
}
});
}
daum.Event.stopEvent(e);
}, gnbToggleBtn));
}
})();
</script>
