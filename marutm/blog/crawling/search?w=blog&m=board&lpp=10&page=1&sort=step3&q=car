(function(){
/* 검색 옵션 기본 객체 구성 */
var so = {
options: [],
relatedElems: [],
urlParams: {
base: {
sd: tabParams.sd,
ed: tabParams.ed,
period: tabParams.period,
page: ''
},
all: {
m: tabParams.m,
sd: '',
ed: '',
period: '',
zone: 'all',
'ccl_derivative': '',
'ccl_commercial': '',
page: '1',
SearchType:'tab',
ResultType:'1',
f: '',
SA: ''
}
}
};
var sourceConfig = {
template: {
id: 'blogOptionsSourceTemplate',
data: {
all: SF.changeUrlParam({f: '', SA: '', p: 1}),
daumsec: SF.changeUrlParam({f: 'section', SA: 'daumsec', p: 1, page: 1}),
tistory: SF.changeUrlParam({f: 'section', SA: 'tistory', p: 1, page: 1}),
selected: ['']
}
},
label: {
all: '출처전체',
daumsec: 'Daum 블로그',
tistory: '티스토리'
},
dataType: 'source'
};
so.options.push(sourceConfig);
so.urlParams.source = {f: '', SA: '', p: 1};
so.relatedElems.push(daum.$('sourceOption'));
var termConfig = {
template: {
id: 'blogOptionsTermTemplate',
data: {
a: SF.changeUrlParam({sd: '', ed: '', period: '', p: '', page: ''}),
d: SF.changeUrlParam({sd: '20130320025219', ed: '20130321025219', period: 'd', p: '', page: ''}),
w: SF.changeUrlParam({sd: '20130314025219', ed: '20130321025219', period: 'w', p: '', page: ''}),
m: SF.changeUrlParam({sd: '20130219025219', ed: '20130321025219', period: 'm', p: '', page: ''}),
u: SF.changeUrlParam({sd: '', ed: '', period: 'u', p: '1' }),
selected: ['']
}
},
label: {
d: '최근 1일',
w: '최근 1주',
m: '최근 1개월',
u: '기간 직접입력'
},
dataType: 'term'
};
so.options.push(termConfig);
so.urlParams.term = {sd: '', ed: '', period: ''};
var menuidConfig = {
template: {
id: 'blogOptionsByMenuIDTemplate',
data: {
link_total: SF.changeUrlParam({m: 'board', page: ''}),
link_tag: SF.changeUrlParam({m: 'tag', page: ''}),
link_local: SF.changeUrlParam({m: 'local', page: ''}),
target: '_blank',
selected: [tabParams.m]
}
},
label: {
tag: '태그',
local: '장소'
},
dataType: 'menuid'
};
so.options.push(menuidConfig);
so.urlParams.menuid = {m: 'board'};
so.relatedElems.push(daum.$('menuIDOption'));
var cclConfig = {
template: {
id: 'blogOptionsCCLTemplate',
data: {
ccl_derivative: "",
ccl_commercial: "",
type: "",
selected: [""]
}
},
label: {
