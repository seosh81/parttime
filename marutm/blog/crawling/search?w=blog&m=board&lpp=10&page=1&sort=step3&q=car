map : function(place, param) {
this.init = function() {
var map_place = $C(place, "map")[0];
if(!param || param == 'undefined') {
$E($C(place, "map")[0]).setWidth($C(place, "wrap_map")[0].clientWidth);
var map_level = 3;
} else {
var map_level = 4;
}
var oldPosX = $E(map_place).getAttribute('local_x');
var oldPosY = $E(map_place).getAttribute('local_y');
var confirm_id = $E(map_place).getAttribute('confirm_id');
var doc_id = $E(map_place).getAttribute('doc_id');
var position = new daum.maps.Congnamul(oldPosX, oldPosY).toCoords();
var newPos = position.toString().replace("(","").replace(")","").replace(" ","").split(",");
var newPosX = newPos[0];
var newPosY = newPos[1];
var marker = new daum.maps.Marker({
position: position
});
this.map = new daum.maps.Map(map_place, {
center: position,
type_ctrl : true,
type_ctrl_pos : {top:-1, right:-2},
mapTypeId: daum.maps.MapTypeId.ROADMAP,
level: map_level,
scrollwheel:false
});
marker.setMap(this.map);
var ul_el = daum.createElement("<ul class=\"ulBlogTabMapType\"></ul>");
map_place.appendChild(ul_el);
this.roadmapBtn = daum.createElement("<li class=\"liBlogTabMap_on\">지도</li>");
ul_el.appendChild(this.roadmapBtn);
this.skymapBtn = daum.createElement("<li class=\"liBlogTabSky\">스카이</li>");
ul_el.appendChild(this.skymapBtn);
daum.addEvent(this.roadmapBtn, "click", (function(event){
this.map.setMapTypeId(daum.maps.MapTypeId.ROADMAP);
$E(this.roadmapBtn).replaceClassName("liBlogTabMap", "liBlogTabMap_on");
$E(this.skymapBtn).replaceClassName("liBlogTabSky_on", "liBlogTabSky");
SF.C.blogTab.clickLog(this.roadmapBtn, doc_id, 30, event);
}).bindAsEventListener(this));
daum.addEvent(this.skymapBtn, "click", (function(event){
this.map.setMapTypeId(daum.maps.MapTypeId.HYBRID);
$E(this.roadmapBtn).replaceClassName("liBlogTabMap_on", "liBlogTabMap");
$E(this.skymapBtn).replaceClassName("liBlogTabSky", "liBlogTabSky_on");
SF.C.blogTab.clickLog(this.skymapBtn, doc_id, 31, event);
}).bindAsEventListener(this));
var ul_el = daum.createElement("<ul class=\"ulBlogTabMapZoom\"></ul>");
map_place.appendChild(ul_el);
var li_el = daum.createElement("<li class=\"liBlogTabPlus\">+</li>");
ul_el.appendChild(li_el);
daum.addEvent(li_el, "click", (function(event){
this.map.setLevel(this.map.getLevel()-1);
SF.C.blogTab.clickLog(li_el, doc_id, 32, event);
}).bindAsEventListener(this));
var li_el = daum.createElement("<li class=\"liBlogTabMinus\">-</li>");
ul_el.appendChild(li_el);
daum.addEvent(li_el, "click", (function(event){
this.map.setLevel(this.map.getLevel()+1);
SF.C.blogTab.clickLog(li_el, doc_id, 33, event);
}).bindAsEventListener(this));
var li_el = daum.createElement("<li class=\"liBlogTabRefresh\">Refresh</li>");
ul_el.appendChild(li_el);
daum.addEvent(li_el, "click", (function(event){
this.map.setLevel(map_level);
this.map.setCenter(new daum.maps.Congnamul(oldPosX, oldPosY).toCoords());
SF.C.blogTab.clickLog(li_el, doc_id, 34, event);
}).bindAsEventListener(this));
var a_el = daum.createElement("<a class=\"aBlogTabMapBig\">전체화면</a>");
map_place.appendChild(a_el);
daum.addEvent(a_el, "mouseover", (function(e){
$E(a_el).addClassName("aBlogTabMapBig_over");
}).bind(this));
daum.addEvent(a_el, "mouseout", (function(e){
$E(a_el).removeClassName("aBlogTabMapBig_over");
}).bind(this));
daum.addEvent(a_el, "click", (function(event){
if(param && param != 'undefined')
{
var place_name = param.name;
}else
{
var place_name = '';
}
window.open('http://local.daum.net/map/index.jsp?srcid=' + confirm_id + '&confirmid=' + confirm_id + '&x=' + newPosX + '&y=' + newPosY +'&map_type=TYPE_MAP&urlLevel=3&q=' + encodeURIComponent(place_name) + '&map_hybrid=false');
SF.C.blogTab.clickLog(a_el, doc_id, 35, event);
}).bindAsEventListener(this));
var mapInfoElem = daum.$$('.map_info', daum.$('br_' + doc_id))[0];
if(mapInfoElem && param && param != 'undefined') {
var mapInfoData = {
docId: doc_id,
url: 'http://place.daum.net/place/Top.do?confirmid=' + param.confirmid,
title: daum.String.cutString(param.name, 26, ".."),
address: (param.address.length > 0) ? param.address : undefined,
phone: (param.tel.length > 0) ? param.tel : undefined,
homepage: (param.homepage.length > 0) ? (param.homepage.length > 50) ? daum.String.cutString(param.homepage, 50, "..") : param.homepage : undefined,
full_homepage: (param.homepage.length > 50) ? param.homepage : undefined,
urlHomepage: (param.homepage.length > 0) ? param.homepage : undefined,
urlRoadview: 'http://local.daum.net/map/index.jsp?srcid=' + param.confirmid + '&confirmid=' + param.confirmid + '&q=' + encodeURIComponent(param.name) + '&rv=on',
urlStoreview: (param.storeview_id && param.storeview_id != "0" ) ? 'http://local.daum.net/map/index.jsp?store=' + param.storeview_id + '&srcid=' + param.confirmid + '&q=' + encodeURIComponent(param.name) : undefined,
urlLocal: 'http://local.daum.net/map/index.jsp?srcid=' + param.confirmid + '&confirmid=' + param.confirmid + '&q=' + encodeURIComponent(param.name),
urlFindPath: 'http://local.daum.net/map/index.jsp?target=car&eX=' + param.x + '&eY=' + param.y + '&eName=' + encodeURIComponent(param.name)
};
mapInfoElem.innerHTML += SF.M.tmpl('mapInfoTemplate', mapInfoData);
}
}
this.init(place, param);
}
};
String.prototype.cutByte = function(index, str){
var cnt = 0;
var newStr = '';
for (var i = 0; i < this.length; i++) {
if (this.charCodeAt(i) > 127){
cnt += 2;
} else {
cnt++;
}
newStr += this.charAt(i);
if(cnt >= index){
return newStr + str;
}
}
return newStr;
};
// 전체 유형
function _jsProcessAllTypeJsonResult(r){
var obj = daum.Ajax.jsonToObject(r.responseText);
var _totalCount;
var entryList;
var templateData = { blogData: [], linkTarget:"_blank" };
switch(tabParams.m){
case "recommend" :
entryList = obj[0].BLOG_RECOMMEND[0].ds;
_totalCount = obj[0].BLOG_RECOMMEND[0].m.c;
break;
case "tag" :
entryList = obj[0].BLOG_TAG[0].ds;
_totalCount = obj[0].BLOG_TAG[0].m.c;
break;
case "local" :
entryList = obj[0].BLOG_LOCAL[0].ds;
_totalCount = obj[0].BLOG_LOCAL[0].m.c;
break;
case "inside" :
entryList = obj[0].BLOG_INSIDE[0].ds;
_totalCount = obj[0].BLOG_INSIDE[0].m.c;
break;
default :
entryList = obj[0].BLOG[0].ds;
_totalCount = obj[0].BLOG[0].m.c;
break;
}
var tempDataStr = new String.Buffer();
if(_totalCount==0){
templateData.blogData = {
enquery: EnQuery
};
if(daum.$("blogColl")){
daum.$("blogColl").style.display="none";
}
if(daum.$("tabNoResultArea")){
daum.$("tabNoResultArea").innerHTML = SF.M.tmpl('blogNoResult', templateData);
daum.$("tabNoResultArea").style.display="block";
}
} else {
var multiThumbArr = [];
var thumbLayerIds = [];
var queryIsDomain = 0;
if(EnQuery.slice(0,13)=="http%3A%2F%2F") {
queryIsDomain = 1;
}
var regdatetime ="";
var logName = "";
var viewonBlogurl = "";
var viewonChkurl = "";
var menuid="blog";
var submenuid="";
if(daum.getParam("m")){
submenuid = daum.getParam("m");
}
var entryLength = 0;
if(_totalCount==1){
entryLength = 1;
} else {
entryLength = entryList.length;
}
var dupEntryCount = 0
for(var i=0, j=entryLength; i<j ; i++){
var entry = entryList[i];
var dupCount = parseInt(entry.dup_info.dup_count, 10) || 0;
dupEntryCount = dupEntryCount + dupCount;
}
var blogResultCount = entryLength + dupEntryCount;
var docIndex = 0;
for(var i=0, j=entryLength; i<j ; i++){
var entry = entryList[i];
var docIdx = i;
templateData.blogData.push(makeResultBody(false, entry, blogResultCount, EnQuery, docIdx));
/*들여쓰기 데이터 시작*/
var dupInfo;
var dupCount = parseInt(entry.dup_info.dup_count, 10) || 0;
var dupEntryList = entry.dup_info.dup_data;
var dupEntryLength = 0;
if ( dupCount != undefined && dupCount > 0 ) {
dupEntryLength = (dupCount == 1) ? 1 : dupEntryList.length;
for(var dup_index=0, len=dupEntryLength; dup_index < len; dup_index++) {
docIndex = docIndex + 1;
var dupEntry = (dupEntryLength == 1) ? dupEntryList : dupEntryList[dup_index];
templateData.blogData.push(makeResultBody(true, dupEntry, blogResultCount, EnQuery, docIdx));
}
}
}
if(daum.$E("blogColl")){
daum.$E("blogColl").style.display="block";
}
if(daum.$E("tabNoResultArea")){
daum.$E("tabNoResultArea").style.display="none";
}
daum.$("blogResultUl").innerHTML = SF.M.tmpl('blogAllResultTemplate', templateData);
if ( $$('.related_thumb', daum.$("blogResultUl")).length > 0 ) {
SF.toggleThumbLayer.init();
}
_jsChgPagingAndCount(_totalCount);
}
var paramStr_m = daum.getParam("m") ? "&m="+daum.getParam("m") : "";
var paramStr_f = daum.getParam("f") ? "&f="+daum.getParam("f") : "";
}
function makeResultBody(isOrigin, entry, blogResultCount, EnQuery, idx) {
var docid = entry.docid || "";
var docids = docids + docid + "||";
var cpid = entry.cpid || "";
var blogurl = entry.blogurl || "";
var site_seq = entry.siteid || "";
var blog_id = entry.blogid || "";
var blogurl_dsp = entry.blogurl_dsp || "";
var posturl_dsp = entry.posturl_dsp_bold || "";
var viewonChkurl = "";
var menuid = "blog";
var allBlogFlag = 0;
var site_name = "";
if(site_seq=="269393") {
site_name = "Daum";
}else if(site_seq=="644103") {
site_name = "allblog";
}
var daum_etc_param = "";
if( entry.blogurl.slice(0, 20) == "http://blog.daum.net" ||  entry.siteid == "794570" ) {
daum_etc_param = daum_etc_param + "?srchid=BR1"+entry.posturl;
}
/* fab thumbs */
var fab_thumbs = [];
if(entry.fab_thumb_path){
if(typeof(entry.fab_thumb_path)=="string"){
fab_thumbs.push(entry.fab_thumb_path);
} else if(typeof(entry.fab_thumb_path)=="object" && entry.fab_thumb_path.length > 1) {
fab_thumbs = entry.fab_thumb_path;
}
}
var hi = 0;
var thumbInfo;
if ( fab_thumbs.length > 0) {
hi = 1;
thumbInfo = {
isVideo: false,
link: entry.posturl + daum_etc_param,
url: fab_thumbs[0],
multiThumb: []
}
if( fab_thumbs.length > 1 ) {
var tempMultiThumbArr = [];
for(var m = 1, ml = fab_thumbs.length; m < ml; m++) {
thumbInfo.multiThumb.push({
index: idx,
url: fab_thumbs[m],
link: entry.posturl+daum_etc_param
});
}
}
}
SF.C.blogTab.clickCodeMap[docid] = {"s":"BL", "a":"BLBL", "d":docid, "pg":tabParams.page, "r": idx+1, "p":"", "e1":blog_id, "e2":site_seq, "rc":blogResultCount, "hi":hi, "ext":"cpid="+cpid};
var clicklog = {
"s": "BL",
"a": "BLBL",
"d": docid,
"pg": tabParams.page || '1',
"r": ( idx + 1 ),
"rc": blogResultCount,
"e1": blog_id,
"e2": site_seq,
"e3": encodeURIComponent(blogurl),
"hi": hi,
"ext": ( "cpid=" + cpid )
};
logs = SF.printLog(clicklog);
var contentInfo;
contentInfo = {
url: entry.posturl,
title: entry.title,
regDateTime: translateTime(entry.regdatetime),
desc: daum.String.trim(entry.description),
isGoodBlog: ( entry.goodblog != "" && entry.goodblog == "y" && daum.getParam("adultquery") != "1" ) ? true : false
};
var gtitQuery = (tabParams.m == "inside") ? EnQuery : blogurl;
if( entry.blogname && entry.blogname != "" ) {
blogNameDisplay = ( daum.String.byteLength(entry.blogname) > 22 ) ? daum.String.cutString(entry.blogname, 19, "..") : entry.blogname;
}else{
blogNameDisplay = '이 블로그 글 전체보기';
}
var displayInfo;
displayInfo = {
url: posturl_dsp,
name: {
url: '?w=' + menuid + '&m=inside&sort=' + tabParams.sort + '&q=' + gtitQuery + '&blogUrl=' + entry.blogurl + '&preq=' + EnQuery + '&onlyone=1&ResultType=2&f=' + tabParams.f + '&SA=' + tabParams.SA,
name: blogNameDisplay
}
};
var insideInfo;
if (tabParams.m != 'inside') {
insideInfo = '?w=' + menuid + '&m=inside&sort=' + tabParams.sort + '&q=' + EnQuery + '&preq=' + EnQuery + '&blogUrl=' + entry.blogurl + '&onlyone=1&ResultType=2&f=' + tabParams.f + '&SA=' + tabParams.SA;
}
var cclInfo;
var cclCommercial = entry.ccl_commercial;
var cclDerivative = entry.ccl_derivative;
if (cclCommercial != "" && cclDerivative != "") {
cclInfo = {
data: _jsMakeCCLIcon(cclCommercial, cclDerivative),
commercial: cclCommercial,
derivative: cclDerivative
};
}
var localInfo;
if ( entry.local_yn.toUpperCase() == "Y" ) {
localInfo = {
confirmID: entry.local_confirm_id,
x: entry.local_map_x,
y: entry.local_map_y
};
}
return {
isOrigin: isOrigin,
docID: docid,
log: logs,
thumb: ( fab_thumbs.length > 0 ) ? thumbInfo : undefined,
content: contentInfo,
display: displayInfo,
inside: insideInfo,
ccl: cclInfo,
local: localInfo,
postUrl: entry.posturl || '',
movieCount: entry.movie_count || '0',
levelRank: entry.blog_level_rank || '0',
blogUrl: entry.blogurl || '',
query: EnQuery
};
}
// 엔트리카운트 및 페이징 변경
function _jsChgPagingAndCount(_totalCount){
var lpp=10;
var rowPerPage=10;
var rowPerBlock=10;
var curPage=1;
var lastC, firstC;
var pageName = "page";
var newLink = document.location.toString();
newLink = _jsMakeNewUrl("sort", tabParams.sort, newLink);
newLink = _jsMakeNewUrl("period", tabParams.period, newLink);
newLink = _jsMakeNewUrl("w", tabParams.w, newLink);
newLink = _jsMakeNewUrl("sd", tabParams.sd, newLink);
newLink = _jsMakeNewUrl("ed", tabParams.ed, newLink);
if(tabParams.m=="blog") tabParams.m = "board";
newLink = _jsMakeNewUrl("m", tabParams.m, newLink);
newLink = _jsMakeNewUrl("ccl_commercial", tabParams.ccl_commercial, newLink);
newLink = _jsMakeNewUrl("ccl_derivative", tabParams.ccl_derivative, newLink);
if(daum.getParam("page_size")) {
lpp = daum.getParam("page_size")*1;
rowPerPage = daum.getParam("page_size")*1;
}
var totalPage =Math.ceil(_totalCount/rowPerPage); // 총 page 수
var totalBlock = Math.ceil(_totalCount/rowPerBlock); // 총 block 수
var curBlock = Math.ceil(curPage/rowPerBlock); // 현재 block 구하기
var startPage = (curPage%rowPerBlock==0) ?  (curPage - (rowPerBlock-1)) : (curPage - curPage%rowPerBlock + 1); // 시작페이지 번호
// 끝페이지 번호
var endPage = (startPage + rowPerBlock - 1);
if(endPage > totalPage) endPage=totalPage;
if(startPage > endPage) startPage=endPage;
// block paging 구하기 start
var startPrevBlockPage = startPage-10;
var endPrevBlockPage = startPage-1;
if(startPrevBlockPage<0) startPrevBlockPage=1;
var startNextBlockPage = endPage+1;
var endNextBlockPage = endPage+10;
if(endNextBlockPage>totalPage) endNextBlockPage=totalPage;
var pagingInstance = new SF.M.paging({
lpp: 10,
totalCount: _totalCount,
currentPage: curPage,
wrapperEl: daum.$$(".paging_comm")[0],
sCode: "BL",
aCode: "PGBL",
pageName: "page",
baseUrl: SF.changeUrlParam({
sort: tabParams.sort,
sd: tabParams.sd,
ed: tabParams.ed,
period: tabParams.period
}, location.search),
additionalParam: {
m: "board"
}
});
pagingInstance.render();
// 카운트 변경
if(_totalCount > (lpp*curPage)){
lastC = lpp * curPage;
firstC = lastC - lpp +1;
}else{
lastC = _totalCount;
firstC = lpp * (curPage-1) +1;
}
daum.$E("resultCntArea").innerHTML = '<em class="nrl g_tit">'+firstC+'-'+lastC+' / ' + SF.M.getSimpleCount(_totalCount) + '</em>';
}
function updateTermOptionStr() {
daum.$("termOptionText").innerHTML = daum.$F("sliderStartDate") + "~" + daum.$F("sliderEndDate");
}
function updateTermOption() {
if(daum.$("termOptionText")){
updateTermOptionStr();
}
if(daum.$("termOption")){
daum.Array.each(daum.$$("#termOption dd a"), function(item, idx){
if(idx==4){
daum.$E(item).removeClassName("link");
daum.$E(item).addClassName("txt");
}else{
daum.$E(item).removeClassName("txt");
daum.$E(item).addClassName("link");
}
});
}
tabParams.sd = daum.$F("sliderStartDate").replace(/\./g,"") + "000000";
tabParams.ed = daum.$F("sliderEndDate").replace(/\./g,"") + "235959";
}
function doSearchByAjax(calltype, addParam){
var targetUrl = "./qsearch";
var param = "w=blog&q="+backQuery+"&uk="+uk;
if(tabParams.period=="u"){
updateTermOption();
}
param += "&sd="+tabParams.sd;
param += "&ed="+tabParams.ed;
if( calltype == "1" && daum.$("sliderStartDate") && daum.$("sliderEndDate") ) {
var sliderStartDateValue = daum.$("sliderStartDate").value;
var sliderEndDateValue = daum.$("sliderEndDate").value;
tabParams.sd = sliderStartDateValue.replace(/\./g,"");
tabParams.ed = sliderEndDateValue.replace(/\./g,"");
}
if(tabParams.sd !="") param += "&sd="+tabParams.sd+"000000";
if(tabParams.ed != "") param += "&ed="+tabParams.ed+"235959";
param += "&sort="+tabParams.sort;
param += "&m="+ tabParams.m;
param += "&onlyone="+ tabParams.onlyone;
param += "&blogUrl="+ tabParams.blogUrl;
param += "&f="+tabParams.f;
if(tabParams.f=="section"){
param += "&SA="+tabParams.SA;
}else{
/*param += "&req=tab";*/
}
if( addParam && addParam != "") {
tabParams.ccl_commercial = addParam.ccl_commercial;
tabParams.ccl_derivative = addParam.ccl_derivative;
}
param += "&ccl_commercial="+tabParams.ccl_commercial+"&ccl_derivative="+tabParams.ccl_derivative;
//기본유형
param += "&page_size=10";
var blogAjax = new daum.Ajax({url:targetUrl,paramString:param,encoding:"utf-8",onfailure:function(r){},ontimeout:function(r){},onsuccess:function(r){_jsProcessAllTypeJsonResult(r);}});
blogAjax.request();
}
function _jsInitTimelineByAjax(){
var targetUrl = "./qsearch";
var param = "w=blog&groupby=regmonth&q="+EnQuery+"&uk="+uk;
var sliderStartDateValue = daum.$("sliderStartDate").value;
var sliderEndDateValue = daum.$("sliderEndDate").value;
param += "&sd="+daum.$("sliderYear1").innerHTML+"0101000000";
param += "&ed="+(daum.$("sliderYear6").innerHTML*1-1)+"1231235959";
param += "&sort="+tabParams.sort;
param += "&m="+ tabParams.m;
param += "&blogUrl="+ tabParams.blogUrl;
param += "&f="+tabParams.f;
param += "&SA="+tabParams.SA;
param += "&ccl_commercial="+tabParams.ccl_commercial+"&ccl_derivative="+tabParams.ccl_derivative;
var newsAjax = new daum.Ajax({url:targetUrl,paramString:param,encoding:"utf-8",onfailure:function(r){},ontimeout:function(r){},onsuccess:function(r){_jsProcessTimelineCountResult(r);}});
newsAjax.request();
}
function _jsProcessTimelineCountResult(r){
var obj = daum.Ajax.jsonToObject(r.responseText);
var entryList;
switch(tabParams.m){
case "recommend" :
var totCnt = obj[0].BLOG_RECOMMEND[0].m.pc;
if( totCnt > 0 ) {
if(obj[0].BLOG_RECOMMEND[0].g && obj[0].BLOG_RECOMMEND[0].g[0]) entryList = obj[0].BLOG_RECOMMEND[0].g[0]["enum"];
}
break;
case "tag" :
var totCnt = obj[0].BLOG_TAG[0].m.pc;
if( totCnt > 0 ) {
if(obj[0].BLOG_TAG[0].g && obj[0].BLOG_TAG[0].g[0] && obj[0].BLOG_TAG[0].g[0]["enum"]) entryList = obj[0].BLOG_TAG[0].g[0]["enum"];
}
break;
case "local" :
var totCnt = obj[0].BLOG_LOCAL[0].m.pc;
if( totCnt > 0 ) {
if(obj[0].BLOG_LOCAL[0].g && obj[0].BLOG_LOCAL[0].g[0] && obj[0].BLOG_LOCAL[0].g[0]["enum"]) entryList = obj[0].BLOG_LOCAL[0].g[0]["enum"];
}
break;
case "inside" :
var totCnt = obj[0].BLOG_INSIDE[0].m.pc;
if( totCnt > 0 ) {
if(obj[0].BLOG_INSIDE[0].g && obj[0].BLOG_INSIDE[0].g[0] && obj[0].BLOG_INSIDE[0].g[0]["enum"]) entryList = obj[0].BLOG_INSIDE[0].g[0]["enum"];
}
break;
default :
var totCnt = obj[0].BLOG[0].m.pc;
if( totCnt > 0 ) {
if(obj[0].BLOG[0].g && obj[0].BLOG[0].g[0] && obj[0].BLOG[0].g[0]["enum"]) entryList = obj[0].BLOG[0].g[0]["enum"];
}
break;
}
daum.Array.each(daum.$$("#sliderChart .stickWrap .stickborder .stickLine"), function(o, i) {
o.style.height = 0;
o.style.marginTop = 40;
});
if(typeof(entryList) != "undefined" ) {
var newsMaxCnt = 0;
var startYear = daum.$("sliderYear1").innerHTML;
var endYear =daum.$("sliderYear6").innerHTML;
var termType = endYear-startYear;
var cntObj = {};
for(var i=0, j=entryList.length; i<j; i++){
var curYear = entryList[i]["@value"].slice(0,4);
var curMonth = entryList[i]["@value"].slice(4,6)*1;
var curCount = entryList[i]["@count"]*1;
var targetMonth = Math.ceil(curMonth/(termType/5))*(termType/5)-(termType/5-1);
if(targetMonth<10) targetMonth = "0"+targetMonth;
if(cntObj[curYear+targetMonth]) {
cntObj[curYear+targetMonth] += curCount;
}else{
cntObj[curYear+targetMonth] = curCount;
}
}
var cntObjLen=0;
for(var prop in cntObj){
if(newsMaxCnt<cntObj[prop]*1) newsMaxCnt = cntObj[prop]*1;
cntObjLen++;
}
var graphConstValue = newsMaxCnt/40;
for(var prop in cntObj){
var cnt = cntObj[prop]*1;
if(cnt>0 && daum.$("st"+prop)){
var heightValue = Math.ceil(cntObj[prop]*1/graphConstValue);
daum.$("st"+prop).style.height = heightValue+"px";
daum.$("st"+prop).style.marginTop = 40-heightValue+"px";
var tooltipTxt = '<div class="cntTooltip hide" id="stw'+prop+'txt"><div class="arrow fL">&nbsp;</div><div id="stw'+prop+'text" class="txt dotum fL">';
var tooltipDate = prop.slice(0,4)+'.'+prop.slice(4,6);
if(termType >5) {
var tmpMonth = prop.slice(4,6)*1+(termType/5-1);
if(tmpMonth<10) tmpMonth = "0"+tmpMonth;
tooltipDate += '~'+tmpMonth;
}
if( daum.$('stw'+prop+'text') ) {
daum.$('stw'+prop+'text').innerHTML = tooltipDate + ' : '+_jsInsertComma(cntObj[prop])+'건';
} else {
daum.$("stw"+prop).innerHTML += tooltipTxt + tooltipDate + ' : '+_jsInsertComma(cntObj[prop])+'건</div><div class="clr"></div></div>';;
}
} else {
daum.$("st"+prop).style.height = "0px";
daum.$("st"+prop).style.marginTop = "40px";
}
}
}
}
function _jsCCLToolTip(obj, c, d, row) {
var html = "";
var layer = daum.$("tabShowLayer");
if( obj != null ) {
var coords = daum.Element.getCoords(obj, daum.$("mArticle"));
var objWidth = obj.clientWidth;
var ls = 0, ts = 0;
if( row == "1" ) {
ts -= 7;
} else if( row == "2") {
ts -= 24;
} else if( row == "3") {
ts -= 47;
}
var l = daum.Browser.ie ? coords.left + objWidth : coords.left + objWidth;
var t = coords.top + ts
html =
'<span class="ccl_box_layer row'+row+'" style="position:absolute;left:'+l+'px;top:'+t+'px;"><dl class="txt_cclayer">'+_jsMakeCCLText(c, d)+'</dl></span>';
if(layer) layer.style.display = "block";
} else {
if(layer) layer.style.display = "none";
}
if(layer) layer.innerHTML = html;
}
function _jsInitCCLOption() {
daum.$("detailCCLOptionBox").style.display = "none";
daum.Element.removeClassName(daum.$("selectCCLBtn"), "Up");
daum.Array.each(daum.$$("#detailOptionList_Wrap ul.detailOptionList_Wrap li>span"), function(e, i) {
e.className = "txtOff";
});
}
function _jsToggleCCLOptionBox() {
var e = daum.Element.toggle(daum.$('detailCCLOptionBox'));
var obj = daum.$("selectCCLBtn");
if( obj ) {
if( e.style.display == "none" ) {
daum.Element.removeClassName(obj, "Up");
} else {
daum.Element.addClassName(obj, "Up");
daum.$("TermSelectSlider").style.display = "none";
daum.Element.addClassName(obj, "Up");
}
}
return false;
}
function _jsMakeCCLText(c, d) {
var viewCCLText = "";
if( c == "1" && d == "A") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd>';
} else if( c == "1" && d == "N") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd><dt>컨텐츠 변경</dt><dd>NO</dd>';
} else if( c == "1" && d == "S") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd><dt>동일조건 변경</dt><dd>YES</dd>';
} else if( c == "0" && d == "A") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd><dt>상업적 이용</dt><dd>NO</dd>';
} else if( c == "0" && d == "N") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd><dt>상업적 이용</dt><dd>NO</dd><dt>컨텐츠 변경</dt><dd>NO</dd>';
} else if( c == "0" && d == "S") {
viewCCLText = '<dt>저작자 표시</dt><dd>YES</dd><dt>상업적 이용</dt><dd>NO</dd><dt>동일조건 변경</dt><dd>YES</dd>';
}
return viewCCLText;
}
function _jsMakeCCLIcon(c, d) {
var data = {txt:"", row:0};
var author = '<span class="ico_comm1 ico_by">저작자표시</span>';
var nonprofit = '<span class="ico_comm1 ico_nc">비영리</span>';
var ban = '<span class="ico_comm1 ico_nb">변경금지</span>';
var assent = '<span class="ico_comm1 ico_sa">동일조건 변경허락</span>';
if( c == "1" && d == "A" ) {
data.txt = author;
data.row = 1;
} else if( c == "1" && d == "N" ) {
data.txt = author + ban;
data.row = 2;
} else if( c == "1" && d == "S" ) {
data.txt = author + assent;
data.row = 2;
} else if( c == "0" && d == "A" ) {
data.txt = author + nonprofit;
data.row = 2;
} else if( c == "0" && d == "N" ) {
data.txt = author + nonprofit + ban;
data.row = 3;
} else if( c == "0" && d == "S" ) {
data.txt = author + nonprofit + assent;
data.row = 3;
}
return data;
}
function _jsSelectedCCLOption(obj, index, isCallAjax) {
var ccl_commercial = "", ccl_derivative = "";
var data = "", result = "", className = "";
var callType = "1";
var d = daum.$("selectedCCLMark");
if( obj ) {
ccl_commercial = obj.getAttribute("ccl_commercial");
ccl_derivative = obj.getAttribute("ccl_derivative");
daum.Array.each(daum.$$("#detailOptionList_Wrap ul.detailOptionList_Wrap li>span"), function(e, i) {
if( i == index ) e.className = "txtOn";
else e.className = "txtOff";
});
data = _jsMakeCCLIcon(ccl_commercial, ccl_derivative);
if( data.txt == "" ) {
className = "";
result = "선택안함";
} else {
className = "c_tit b";
result = data.txt + "&nbsp;선택";
}
} else {
result = "";
className = "";
_jsInitCCLOption();
}
d.className = className;
d.innerHTML = result;
if( tabParams.sd == "" && tabParams.ed == "" && tabParams.period == "") callType = "0";
if( isCallAjax ) doSearchByAjax(callType, {"ccl_commercial": ccl_commercial, "ccl_derivative": ccl_derivative});
}
var _jsIntervalTooltip = function(boxId, wrapId, pos) {
this.boxId = boxId;
this.wrapId = wrapId;
this.CLOSE_INTERVAL = 250;
this.nowShowLayer = null;
this.isShowPreview = false;
this.intervalId = -1;
this.pos = pos;
this.init();
}
_jsIntervalTooltip.prototype = {
init: function() {
var that = this;
daum.Event.addEvent(daum.$(this.boxId), "mouseover", function(e) {
var tg = (window.event) ? e.srcElement : e.target;
if (tg && tg.id != that.boxId) return;
that.nowShowLayer = tg;
});
daum.Event.addEvent(daum.$(this.boxId), "mouseout", function(e) {
var tg = (window.event) ? e.srcElement : e.target;
if (tg && tg.id != that.boxId) return;
var reltg = (e.relatedTarget) ? e.relatedTarget : e.toElement;
while (reltg != tg && reltg.nodeName != 'BODY')
reltg= reltg.parentNode;
if (reltg== tg) return;
that.hide();
});
},
hide: function() {
this.nowShowLayer = null;
this.isShowPreview = false;
window.clearTimeout(this.intervalId);
daum.$(this.boxId).style.display = "none";
}
}
domReady(function() {
new _jsIntervalTooltip("CCLHelpLayer", "CCLHelpWrapLayer", {"top":-54, "left":-57}).init();
daum.Array.each(daum.$$("#detailOptionList_Wrap ul.detailOptionList_Wrap li a"), function(o, i) {
var index = i;
var obj = o;
var ccl_commercial = obj.getAttribute("ccl_commercial") || "";
var ccl_derivative = obj.getAttribute("ccl_derivative") || "";
daum.Event.addEvent(o, "click", function(e) {
_jsSelectedCCLOption(obj, index, true);
smartLog(null, 's=BL&a=SSBL&pg=1&rc=1&r=1&p='+(12+index), '');
gUSRXLink(null, 'SSBL', '1', (12+index), '', '', '', '', '', '', '', '1', e);
if( e.preventDefault ) e.preventDefault();
else e.returnValue = false;
});
if( ccl_commercial == tabParams.ccl_commercial && ccl_derivative == tabParams.ccl_derivative ) {
_jsSelectedCCLOption(obj, index, false);
}
});
});
