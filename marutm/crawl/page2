}



function viewMyMemberLevel() {
	open_window("/MemberLevelView.nhn?m=mylevelview&clubid=10298136", "mylevelview", 450, 300, "");
}
	


function changeBoardType(boardtype) {
    
    if (boardtype == 'B' || boardtype == 'W') {
        nhn.MessageBox.alert('카페 멤버만 사용할 수 있는 메뉴입니다');
        return;
    }
    
    dynamicParamLink("/ArticleList.nhn?search.clubid=10298136&search.menuid=&search.boardtype=" + boardtype);
}


function sortBy(type, e){
  $("sortBy").value = type;
  nhn.search.goSearch(e || window.event);
}

function findArticleHeadList(){
    var selectedValue = document.getElementById("headSelectBox").value;
    document.location.href = "/ArticleList.nhn?search.clubid=10298136&search.menuid=&search.boardtype=L&userDisplay=15&search.headid=" + selectedValue;  
}





function changeListSize(display){
	dynamicParamLink("/ArticleList.nhn?search.clubid=10298136&search.boardtype=L&search.menuid=&search.questionTab=A&search.marketBoardTab=D&search.specialmenutype=&userDisplay=" + display);
}


function popupHelp(){
    open_window("/common/editor/market_help/basic.jsp", "popupHelp", 790, 545," resizable=no");
}


function commentPopup(articleid, referrerall) {
	var url = 'http://cafe.naver.com/remonterrace/' + articleid + '/comment';
	var feature = "location=no,status=no,toolbar=no,menubar=no,resizable=yes,scrollbars=yes";
	
	if(referrerall != null ) {
		openWindow(url + '?referrerAllArticles=' + referrerall, 'CommentPopup', 780, 682, feature);
	} else {
		openWindow(url + '?referrerAllArticles=true', 'CommentPopup', 780, 682, feature);	
	}
}
