LH.add("writelcs()");
LH.add("checkParent()");

LH.add("initHeadListLayerForQuestionAnswerBoard()");


function checkParent() {
    if (parent && parent == self) {
        document.location.href = "http://cafe.naver.com/remonterrace.cafe?iframe_url=" + document.location.href.replace(/^http:\/\/(cafe|cafe2).naver.com/gi, "").replace(/\?/gi, "%3F").replace(/&/gi,"%26");
    }
}

checkParent();

