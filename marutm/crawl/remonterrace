	{
	    moveIframePage('/CafeProfileView.nhn?clubid=10298136');
	}
	
	// 카페 랭킹
	function cafeRanking()
	{
	    moveIframePage('/CafeRankingCafeView.nhn?clubid=10298136');
	}
	
