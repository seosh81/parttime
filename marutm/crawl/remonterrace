	function commentPopup(articleid) {
		var url = 'http://cafe.naver.com/remonterrace/' + articleid + '/comment';
		var feature = "location=no,status=no,toolbar=no,menubar=no,resizable=yes,scrollbars=yes";
		openWindow(url + '?referrerAllArticles=true', 'CommentPopup', 780, 682, feature);
	}
