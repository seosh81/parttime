<script type="text/javascript">
document.domain = "naver.com";

function notiAlert(frmStr)
{
    open_window("", frmStr, 100, 100, "")
    var frm = eval("document." + frmStr);   
    frm.target = frmStr;
    try{
        frm.submit();
    }catch(e){}
}









function visit() {

     var ajax = new Ajax("/MyCafeVisit.nhn", {
             suspend : "true",         
             method : "POST",
             params : { "clubid":10298136,"usingCounter":true,"ceremonyCheckNeeded":false },      
             onLoad : cafeVisitCB
     });
     ajax.request();

}
function cafeVisitCB(res) { 
	if(res.status==200) {
		try{ 
	
			var counter = eval('(' + res.responseText + ')');
			
			if(counter == null || counter == 'undefined') {
				return false;
			}
			
		
				if(counter.totalVisitCount != null) { $("totalVisitCount").innerHTML = counter.totalVisitCount; }
				if(counter.todayVisitCount != null) { $("todayVisitCount").innerHTML = counter.todayVisitCount; }
		
		
		
				if(counter.visitEventResult != null && counter.visitEventResult.win == 'true') {
					open_window('/CafeEventPop.nhn?clubid=10298136&eventtype=2&eventresult='+counter.visitEventResult.condition+'&eventarticle='+counter.visitEventResult.eventArticleid, 'cafe_event_popup', 330, 250);
				}
				if(counter.ceremonyPassed == 'true') {
					open_window("/CafeCeremonyProcess.nhn?clubid=10298136&m=pass","cafe_ceremony_pass", 450, 200);
				}
		       
		
			if(counter.notiList != null) {
				for (var i = 0; i < counter.notiList.length; i++) {
					if (counter.notiList[i].exposeAll == 'true' || counter.notiList[i].exposeCafe == 'true') {			
						document.notifrm.action = counter.notiList[i].param2;
						document.notifrm.noticode.value = counter.notiList[i].noticode;
						document.notifrm.comment.innerText = counter.notiList[i].message;
						
						notiAlert('notifrm');
					}
				}
			}
		}catch(e){
			JEagleEyeClient.raiseError(e, this, {
        		message : "response:" + res.responseText}
        	);
		}  
	} 
} 

LHajax.add("visit()");

    
 

    
 

Function.prototype.bind = function(){
    var f = this, a = $A(arguments), object = a.shift();
    return function(){
    f.apply(object, a.concat($A(arguments)));
    }
};
  
function callAppView(componentid, appId) {  
    var ajax = new Ajax("/AppCafeViewAjax.nhn?clubid=10298136&appId="+appId, {
            suspend : "true",
            onLoad : appViewCallBack.bind(window,componentid,appId)
    });
    ajax.request();
}


function appViewCallBack(componentid, appId, res) {
    $(componentid).innerHTML = ''; 
    $(componentid).innerHTML = res.responseText;

    try{
        parentResizeIframe('cafe_main');
    }catch(e){}
}

 






 


















var cafe_watch = '|undefined|undefined|undefined';
if(cafe_watch.indexOf("undefined")>-1)
    cafe_watch = 'digital_064_05|4|7F7F7F|FFFFFF';
var cafe_watch_name = cafe_watch.split('|')[0];
var cafe_watch_font = cafe_watch.split('|')[1];
var cafe_watch_color = cafe_watch.split('|')[2];
var cafe_watch_bgcolor = cafe_watch.split('|')[3];

var watch_vars = {
    req_url   : encodeURIComponent('http://bridge.cafe.naver.com/ContentWidget.nhn?m=watch&city=KR004&'),
    fontData  : cafe_watch_font,
    fontColor : cafe_watch_color,
    bgColor   : cafe_watch_bgcolor,
    url       : '/common/flash/watch/',
    name      : cafe_watch_name,
    setUrl    : function(){
        return this.url + this.name +'.swf';
    },
    flashvars : function(){
        return "req_url=" + this.req_url +"&fontData=" + this.fontData +"&fontColor=" + this.fontColor + "&bgColor=" + this.bgColor;
    },
    height    : function(){
        var n = this.name; 
        h = n.match(/_\s*(.*?)_/)[1];
        h = h.replace(/^[0]/,'');
        return h;
    }
};

function initWatchWidget()
{
    new widget('widget-watch', {
        setUrl : watch_vars.setUrl(),
        getData : 'flash',
        flashvars : watch_vars.flashvars(),     
        height : watch_vars.height()
    });
    $("widget-watch").style.height=watch_vars.height()+"px";
    try{
        parentResizeIframe('cafe_main');
    }catch(e){}
}

LHajax.add("initWatchWidget()");





function initWeatherWidget()
{
    new widget('widget-weather', {
        setUrl : 'http://bridge.cafe.naver.com/ContentWidget.nhn?m=weather&iconid=1&type=1&area=11B10101&'
    });
}

LHajax.add("initWeatherWidget()");

    

