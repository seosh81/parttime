<script type="text/javascript">
var cafeappInstallIntroLayer = {
    init : function(pUserLoggedIn) {
        this.cafeappInstallIntroLayer = $('cafeappInstallIntroLayer');
        this.cafeappInstallIntroLayerSmsBtn = $('cafeappInstallIntroLayerSmsBtn');
        this.cafeappInstallIntroLayerCloseBtn = $('cafeappInstallIntroLayerCloseBtn');
        this.cafeappInstallIntroLayerCloseXBtn = $('cafeappInstallIntroLayerCloseXBtn');
        this.userloggedIn = pUserLoggedIn;
        this.bindEvent();
    },
    bindEvent : function() {
        Event.register(this.cafeappInstallIntroLayerSmsBtn, 'click', this.popupSms.bindForEvent(this));
        Event.register(this.cafeappInstallIntroLayerCloseBtn, 'click', this.hideLayer.bindForEvent(this));
        Event.register(this.cafeappInstallIntroLayerCloseXBtn, 'click', this.hideLayer.bindForEvent(this));
    },    
    hideLayer : function(we) {
        oCL.hide(this.cafeappInstallIntroLayer);
        we.stop();
    },    
    popupSms : function(we) {
    	openPopupSendURL();
        oCL.hide(this.cafeappInstallIntroLayer);
        we.stop();
    },
    showLayer : function(event, nTop, nLeft) {
    	
    	if(!this.userloggedIn) {
    		popupLoginPage(document.location);
    		return;
    	}
    	var oAjax = new Ajax('/CafeAppInstallPushAlim.nhn', {method : "GET"});
    	oCL.show(this.cafeappInstallIntroLayer, event, nTop, nLeft);
    	if(event != null) {
    		Event.ready(event).stop();	
    	}
    }
};
cafeappInstallIntroLayer.init(false);
</script>
<script type="text/javascript">
	 function resizeIfr(id, nMinHeight) {
		var minHeight, doc, h, s;
		minHeight = nMinHeight || 10;
		var elIframe = $(id);
		try{doc = elIframe.contentWindow.document;}catch(me){return;}
	
		if (doc.location.href == 'about:blank') {
			elIframe.style.height = minHeight+'px';
			return;
		}
	
		if (/MSIE/.test(navigator.userAgent)) {
			h = doc.body.scrollHeight;
		} else {
			s = doc.body.appendChild(document.createElement('DIV'));
			s.style.clear = 'both';
	
			h = s.offsetTop;
			s.parentNode.removeChild(s);
		}
	
		if (h < minHeight) h = minHeight;
	
		elIframe.height = h;
		elIframe.style.height = h + 'px';
		minHeight = doc = h = s = null;
	}

	function openPopupWindow(url, name, width, height) {
		var popup, option = "scrollbars=yes,resizable=yes,toolbar=no,status=no,menubar=no,location=no";

		if (width < window.screen.width && height < window.screen.height) {
		    var left = window.screen.width  - width;
		    var top = Math.ceil((window.screen.height - height) / 2);
		
		    popup = window.open(url, name, option + ",width=" + width + ",height=" + height + ",left=" + left + ",top=" + top);
		} else {
			popup = window.open(url, name, option + ",width=" + width + ",height=" + height);
		}

		popup.focus();
	}
	
	function openPopupQuestion() {
		var url = 'http://help.naver.com/ops/step1/query.nhn';
		window.open(url, "hiddenIframe","scrollbars=no,directories=no,resizable=no,toolbar=no,status=no,menubar=no,location=no,width=1000,height=700,left=0,top=0");
	}

	function findContentUrl() {
		var contentUrl;
		try {
			contentUrl = $('cafe_main').contentWindow.window.location.href;
			return contentUrl;
		} catch(e) {}
	}

	var ToggleCafeGNBLayer = {
		init : function() {
			this.elJoin = $("au_join-cafe");
			this.elListLayer = $("join-cafe-iframe");
            this.elListLoginLayer = $("layer_login");			
			this.elListButton = this.elJoin.getElementsByTagName("img")[0];

			this.elGo = $("au_go");
			this.elGoLayer = $("layer-go");
			this.elGoButton = this.elGo.getElementsByTagName("img")[0];
			
			this.elCurrent = { layer : null, button : null };
			this.bEventAttached = false;
			this.bJindo2 = typeof $Jindo != "undefined" ? true : false;
			this.attachEvent(true);
		},
		attachEvent : function(bInit) {
			if(bInit) {
				if(this.bJindo2) {
					$Fn(this.clickArea, this).attach(this.elJoin, "click");
					$Fn(this.clickArea, this).attach(this.elGo, "click");
					$Fn(this.hide, this).attach(document, "click");
				} else {
					Event.register(this.elJoin, "click", this.clickArea.bindForEvent(this));
					Event.register(this.elGo, "click", this.clickArea.bindForEvent(this));
					Event.register(document, "click", this.hide.bindForEvent(this));
				}
			} else if(!this.bJindo2) {
				this.elFrame = $("cafe_main");

				if(this.elFrame) {
					this.elFrameLocation = this.elFrame && this.elFrame.contentWindow && this.elFrame.contentWindow.location.toString() || "";
					this.frameEvent();
				}
				this.bEventAttached = true;
			}
		},
		frameEvent : function(bDetach) {
			var elEditor = this.elFrame && this.elFrame.contentWindow.oEditor;
			
			Event[bDetach ? "unregister" : "register"](this.elFrame.contentWindow.document, "click",this.hide.bindForEvent(this));
				
			
			if(elEditor) {
				Event[bDetach ? "unregister" : "register"](elEditor.getDocument(), "click",this.hide.bindForEvent(this));
			}
		},
		clickArea : function(oEvent) {
			var el = oEvent.target || oEvent.srcElement || oEvent.element;

			if(!/(a|img)/i.test(el.tagName)) {
				return;
			}
			
			var bChild = this.bJindo2 ? $Element(el).isChildOf(this.elJoin) : Element.has(this.elJoin, el);
			this.show(bChild);

			if(this.bJindo2) {
				oEvent.stop();
			} else {
				Event.stop(oEvent);

				if(!this.bEventAttached) {
					this.attachEvent();
				}
			}
		},
		show : function(bJoin) {
			var sType = bJoin ? "List" : "Go";
			var	elLayer = (true && bJoin) ? this["el" + sType + "LoginLayer"] : this["el"+ sType +"Layer"];
			var	elButton = this["el"+ sType +"Button"];
			var bVisible = this.bJindo2 ? $Element(elLayer).visible() : Element.visible(elLayer);
			
			if (false) {
	            if(bJoin && elLayer.src == "") {
	                elLayer.src = "/MyCafeListGNBView.nhn";
	            }
			}
			
            this.hide({ target : elLayer });
            this.bJindo2 ? $Element(elLayer).toggle() : Element.toggle(elLayer);
			this.elCurrent = { layer : elLayer, button : elButton };

			elButton.className = bVisible ? elButton.className.replace("up","down") : elButton.className.replace("down","up");
			
		},
		hide : function(oEvent) {
			var el = oEvent && (oEvent.target || oEvent.srcElement);

			if(this.elCurrent.layer && this.elCurrent.layer != el) {
				this.elCurrent.layer.style.display = "none";
				this.elCurrent.button.className = this.elCurrent.button.className.replace("up","down");
			}

			// iframe location이 다른 경우에 클릭 이벤트를 재설정한다.
			if(this.elFrameLocation && this.elFrameLocation != this.elFrame.contentWindow.location) {
				this.frameEvent(this);
				this.elFrameLocation = this.elFrame.contentWindow.location.toString();
				this.frameEvent();
			}
		}
	};
	ToggleCafeGNBLayer.init();
	

</script>
			
		</div>
		
		<!--  상단 영역 -->	
		
			
			





<script type="text/javascript">
// 탑스킨 사이즈 제한
function resizeTopSkin(img) {
    if(!img || img.src.indexOf(".png")>-1 ||img.src.indexOf(".PNG")>-1) return; 
    if(img.height>300) img.height="300";
    if(img.width>956) img.width="956";
}

function updateMoveMenu() {
	if (typeof oMenuMoveUpdater == "undefined") {
		return;
	}
	
	oMenuMoveUpdater.update(null);
}
</script>


	
	
	
	
