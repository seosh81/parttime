function suggestJoin()
{
	

    var clubname = "레몬테라스 [인테리어,리폼,DIY,요리,결혼,육아]";
    if (confirm('카페멤버만 이용할 수 있는 기능입니다.\n' + clubname +' 카페에 가입하시겠습니까?')){
		joinCafe();
    }
}






