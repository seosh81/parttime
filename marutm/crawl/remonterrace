			nMenuId : 127,
			bBadMenu : false,
			sMenuType : "B",
			sBoardType : "L",
			bHasArticleForm : true,
			bHasTemplate : false
		});
		
		oMenus.add(oMenu);
	
		var oMenu = new Cafe.menu.Menu({
			nMenuId : 165,
			bBadMenu : false,
			sMenuType : "B",
			sBoardType : "L",
			bHasArticleForm : true,
			bHasTemplate : false
