	function oldchatting() {
		var chatframe = $C("iframe");
		chatframe.height = "0";
		document.body.appendChild(chatframe);
		chatframe.src = "/ChatCafe.nhn?clubid=10298136";
	}
	
		// 회원정보 변경
	function cafeMemberInfoEdit(event){
		var we = Event.ready(window.event || event);
		if(we.element.tagName.toLowerCase() != "a" ){
			// height 780,830
			open_window("/CafeMemberInfo.nhn?clubid=10298136&memberid=&m=view", "popup", 500, 780, "toolbar=0,menubar=0,scrollbars=yes,resizable=yes");
		}	    
	}
