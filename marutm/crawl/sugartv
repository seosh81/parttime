    function toggleFirstList()
  { 
     if($("rankarticle-commentcount-list").style.display=="none")
     {	
	  Element.show("rankarticle-commentcount-list");
	  Element.hide("rankarticle-commentcount-second-list");
	  Element.show("pre-commentcount-disable");
	  Element.show("next-commentcount-useable");
	  Element.hide("pre-commentcount-useable");
	  Element.hide("next-commentcount-disable");
	}
  }
  
  function toggleSecondList()
  {  
     if($("rankarticle-commentcount-second-list").style.display=="none")
     {
	  Element.show("rankarticle-commentcount-second-list");
  	  Element.hide("rankarticle-commentcount-list");
  	  Element.show("pre-commentcount-useable");
	  Element.show("next-commentcount-disable");
	  Element.hide("pre-commentcount-disable");
	  Element.hide("next-commentcount-useable");
  	  
	}
  }

