<script type="text/javascript" src="/javascript/URLEncodingMap_MS949.js?1311297509000"></script>

<script type="text/javascript" src="/javascript/URLEncoder.js?1311297509000"></script>

<script type="text/javascript">
var searchParentFrame = null;
function searchBoard()
{
	searchParentFrame = document.frmBoardSearch;
	
 	if (searchParentFrame.query.value.split(" ").join("")=="") 
    {
    	alert("검색어를 입력하세요.");
        searchParentFrame.query.focus();
        return;
    }

    //CAFESUS-4750  공통 금칙어 필터/팝업 삭제        
    //var wordcheckframe = document.createElement("iframe");
    //wordcheckframe.width = "0";
    //wordcheckframe.height = "0";
    //document.body.appendChild(wordcheckframe);
    //wordcheckframe.src = "/SectionSearchWordCheck.nhn";
    searchFrmAfter(searchParentFrame);
    
    return;
}

function searchFrmAfter(frm)
{
    $("cafe_main").src = "/ArticleSearchList.nhn?search.clubid=24176653&search.searchBy=0&search.query=" + URLEncoder.encode(frm.query.value,"MS949");
}

</script>
<div id="cafe-search">
	<form name="frmBoardSearch" action="" onsubmit="return false;" autocomplete="off">
	   <input type=text name="query" style="ime-mode:active" onkeydown="if (event.keyCode == 13) {searchBoard();}"><a href="#" onclick="searchBoard();return false;"><img src="http://cafeimgs.naver.net/cafe4/hidden.gif" width="42" height="21" alt="검색" class="btn-search-green"></a>
	</form>
</div>

