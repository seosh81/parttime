<script type="text/javascript">    
var memberType = new Array("join","birthday","visit");
var isGetMemberNewsjoin = false;
var isGetMemberNewsbirthday = false;
var isGetMemberNewsvisit = false;

// 멤버소식 탭 전환
function showMembernews(type)
{ 
    callMemberNews(type);
    
    try{
    	var etc = {};
    	if(type == "birthday") {
    		etc["sti"] = "cafe_membernews_birthday";
    	} else if (type == "join") {
    		etc["sti"] = "cafe_membernews_join";
    	} else {
    		etc["sti"] = "cafe_membernews";
    	}
    	lcs_do(etc);
    } catch(e) {}
}

// 멤버소식 조회
function callMemberNews(type)
{
  if(! eval("isGetMemberNews"+type) )
  { 
    var ajax = new Ajax("/MemberNews.nhn?clubid=24176653&type="+type, {
      suspend : "false",        
      onLoad : memberNewsCB
    });
    ajax.request();
    
  }else{
     $("member-news").innerHTML = $("member-"+type).innerHTML;
  }
  
  
}
// 멤버소식 콜백
function memberNewsCB(res)
{
  if(res.status==200)
  {
    var type = res.getResponseHeader("newstype");
    $("member-news").innerHTML = "";
    $("member-"+type+"-list").innerHTML = res.responseText;
    eval("isGetMemberNews"+type+"=true");
    $("member-news").innerHTML = $("member-"+type).innerHTML;  
  } else {
     alert("멤버소식 조회를 실패했습니다.");
  }
}

// 방문멤버삭제
function deleteVisit(memberid)
{ 
  var ajax = new Ajax("/CafeVisitMemberDelete.nhn?clubid=24176653&memberid=", {
      suspend : "false",        
      onLoad : deleteVisitCB
  });
  ajax.request();
}

// 방문멤버삭제 콜백
function deleteVisitCB(res)
{
  if(res.status==200)
  {
    isGetMemberNewsvisit = false;
    callMemberNews('visit');
  }
  else
  {
    alert("방문기록삭제를 실패했습니다.\n다시 시도해주세요");
  }
}

  
  function firstVisitList()
  { 
     if($("first-visit-page").style.display=="none")
     {  
    Element.show("first-visit-page");
    Element.hide("second-visit-page");
    Element.show("pre-visit-disable");
    Element.show("next-visit-useable");
    Element.hide("pre-visit-useable");
    Element.hide("next-visit-disable");
  }
  }
  
  function secondVisitList()
  {  
     if($("second-visit-page").style.display=="none")
     {
    Element.show("second-visit-page");
      Element.hide("first-visit-page");
      Element.show("pre-visit-useable");
    Element.show("next-visit-disable");
    Element.hide("pre-visit-disable");
    Element.hide("next-visit-useable");
      
  }
  }
  
  
  function firstJoinList()
  { 
     if($("first-join-page").style.display=="none")
     {  
    Element.show("first-join-page");
    Element.hide("second-join-page");
    Element.show("pre-join-disable");
    Element.show("next-join-useable");
    Element.hide("pre-join-useable");
    Element.hide("next-join-disable");
  }
  }
  
  function secondJoinList()
  {  
     if($("second-join-page").style.display=="none")
     {
    Element.show("second-join-page");
      Element.hide("first-join-page");
      Element.show("pre-join-useable");
    Element.show("next-join-disable");
    Element.hide("pre-join-disable");
    Element.hide("next-join-useable");
      
  }
  }
  
  
  function firstBirthList()
  { 
     if($("first-birth-page").style.display=="none")
     {  
    Element.show("first-birth-page");
    Element.hide("second-birth-page");
    Element.show("pre-birth-disable");
    Element.show("next-birth-useable");
    Element.hide("pre-birth-useable");
    Element.hide("next-birth-disable");
  }
  }
  
  function secondBirthList()
  {  
     if($("second-birth-page").style.display=="none")
     {
    Element.show("second-birth-page");
      Element.hide("first-birth-page");
      Element.show("pre-birth-useable");
    Element.show("next-birth-disable");
    Element.hide("pre-birth-disable");
    Element.hide("next-birth-useable");
      
     }
  }
</script>
						
						 
					
						

