<script type="text/javascript">

var rankArticleType = new Array("upcount","commentcount");
var isGetRankArticleupcount = true;
var isGetRankArticlecommentcount = false;

function showRankArticle(type)
{	
    callRankArticle(type);
}

// 게시물 랭킹 조회
function callRankArticle(type)
{
	if(! eval("isGetRankArticle"+type) )
	{	
		var ajax = new Ajax("/RankArticle.nhn?clubid=24176653&type="+type, {
				suspend : "false",				
				onLoad : rankArticleCB
		});
		ajax.request();
	}else
	{
        $("rank-article").innerHTML = $("rankarticle-"+type).innerHTML;
        try{
        	var etc = {};
        	etc["sti"] = "cafe_rankarticle";
        	lcs_do(etc);
        }catch(e) {} 
	}
}

// 게시물랭킹 콜백
function rankArticleCB(res)
{
	if(res.status==200)
	{
		var type = res.getResponseHeader("ranktype");
		$("rankarticle-"+type+"-list").innerHTML = res.responseText;
		eval("isGetRankArticle"+type+"=true");
        $("rank-article").innerHTML = $("rankarticle-"+type).innerHTML;
      
        try{
        	var etc = {};
        	etc["sti"] = "cafe_rankarticle";
        	lcs_do(etc);
        }catch(e) {} 
    }else
    {
       alert("게시물랭킹 조회를 실패했습니다.");
    }
}

  function firstRankList()
  { 
     if($("first-rankup-list").style.display=="none")
     {	
	  Element.show("first-rankup-list");
	  Element.hide("second-rankup-list");
	  Element.show("pre-rank-disable");
	  Element.show("next-rank-useable");
	  Element.hide("pre-rank-useable");
	  Element.hide("next-rank-disable");
	}
  }
  
  function secondRankList()
  {  
     if($("second-rankup-list").style.display=="none")
     {
	  Element.show("second-rankup-list");
  	  Element.hide("first-rankup-list");
  	  Element.show("pre-rank-useable");
	  Element.show("next-rank-disable");
	  Element.hide("pre-rank-disable");
	  Element.hide("next-rank-useable");
  	  
	}
