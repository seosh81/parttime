<script type="text/javascript">

var nsc = "cafe.mycafe";


(function() {
	var elCafeCss = $("cafeCss"),
		elParent = parent.document.getElementById("cafeCss"),
		re = /body[^}]*}/i, sReplace = "body {background-color:transparent}";
	
	if(elCafeCss.styleSheet) { //IE
		elCafeCss.styleSheet.cssText = elParent.styleSheet.cssText.replace(re, sReplace);
	} else {  //FF
       var replaceString = elParent.innerHTML.replace(re, sReplace); 

		try {
			elCafeCss.innerHTML = replaceString; 
       	} catch(e) { //Safari
    		elCafeCss.innerText = replaceString;
       	} 
	}
})();

var oCL = new Ju.controlLayer({
	onShow : function(oElement){
		
		parentResizeIframe('cafe_main', 'true');
	},
	onHide : function(){
		
	}
});


function dynamicParamLink(reqURL, event){
	var noticeHidden = $('notice_hidden');

	
	if(event) {	
		if(event.ctrlKey || event.metaKey) {
			return;
		}
	}
	 
	if(noticeHidden == null) {
		document.location.href = reqURL;
		return false;
	} 	
	
	var isNoticeClosed = $('notice_hidden').checked;
	 
	if(isNoticeClosed) {
		reqURL = reqURL + "&noticeHidden=true";
		document.location.href = reqURL ;
		return false;
	}
	
	document.location.href = reqURL;
	return false;
}


function findArticleHeadListForBoardType(headid){
    dynamicParamLink("/ArticleList.nhn?search.clubid=10298136&search.menuid=&search.boardtype=L&userDisplay=15&search.headid=" + headid);  
}

function initHeadListLayerForQuestionAnswerBoard() {
	var listHtml = "<ul>";
	listHtml += "    <li><a href=\"javascript:findArticleHeadListForBoardType();\"><span>전체보기</span></a></li>";
	
	
	
	document.getElementById("headListLayer").innerHTML = listHtml;
}

var LH = new LH_create();
window.onload = LH_exec;

