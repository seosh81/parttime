<script type="text/javascript">

nhn.search.init(false, "http://cafe.naver.com", 10298136, 15, false, null);





function showLayer(obj, layerName){
	var pos = Element.realPos(obj);
	var elLayer = $(layerName);

	oCL.show(layerName);
	if( elLayer.style.display != 'none' ) {
		elLayer.style.top = (pos.top + 14 ) + 'px';
		elLayer.style.left = (pos.left - 8 ) + 'px';
	}
}


function applyDisplayNotice()
{
	var isNoticeClosed = $('notice_hidden').checked;
	
	toggleDisplayNotice(isNoticeClosed);
}


function toggleDisplayNotice(isNoticeClosed) {
	var upperArticleListArea = $('upperArticleList');
	
	if(isNoticeClosed) {		
		var elements = cssquery("._noticeArticle", upperArticleListArea);
		var noticeCount = elements.length;
		for(i=0; i < noticeCount ; i++) {
			var obj = elements[i];
			if(obj != null) {
				 obj.style.display = "none";
			}
		}
	} else {
		var elements = cssquery("._noticeArticle", upperArticleListArea);
		var noticeCount = elements.length;
		for(i=0; i < noticeCount ; i++) {
			var obj = elements[i];
			if(obj != null) {
				 obj.style.display = "";
			}
		}
	}
}


function initDisplayNotice() {
	var noticeHidden = $('notice_hidden');

	//공지 숨기기 버튼 존재하지 않는지 valid check
	if(noticeHidden == null || typeof noticeHidden == "undefined") {
		return;
	} 

	//noticeHidden 은 현재 레이아웃을 사용하는 Action에 정의되어야 한다. 
	if(false) {
		$('notice_hidden').checked = true;
	}else{
		$('notice_hidden').checked = false;
	}
	toggleDisplayNotice(false);
}

LH.add(initDisplayNotice());




(function() {
	Event.register(window, "load", function() {
		Tablet.editing.ClickAlert()
			.check({
				bAccessTabletPC : false,
				bReadOnlyStatus : false,
				bMarketBoard : false,
				bHasArticleForm : false,
				bHasTemplate : true
			})
			.click("writeFormBtn").run(function() {
				writeArticle();
			})
			.click(cssquery("a[name=modifyLink]")).run(function() {
				// empty
			});
	});
})();

var bTablet = false;
var oTabletHandler = new TabletHandler(bTablet);



function findMenuName() {
	if(true) {
		if("" == "M") {
			return "동영상 모아보기";
		}else if("" == "P") {
			return "이미지 모아보기";
		}
		return "전체글보기";	
	} else {
		return "";
	}
}

var cafenameTitle = "레몬테라스 [인테리어,리폼,DIY,요리,결혼,육아]";
var pageTitle = findMenuName() +','+ cafenameTitle + ' : ' +  '네이버 카페' ;
LH.add(changeTitle(pageTitle));

</script>



