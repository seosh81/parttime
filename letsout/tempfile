var HappyBeanLayer = $Class({
	oTid : null,
	$init : function() {
		if (isHappyBeanLeverage) {
			this.oTid = setInterval($Fn(function(){
				if($Ajax.SWFRequest.activeFlash){
					this.callHappyBeanLeverage()
					clearInterval(this.oTid);
				}
			},this).bind(),500)
		//	setTimeout(this.callHappyBeanLeverage,900);
		}
	},

	callHappyBeanLeverage : function () {
		new $Ajax(hbeanLeverageApiURL, {
			type : 'flash',
			method : 'get',
			sendheader : false,
			decode : false,
			onload : function(res) {
				var oResult = eval('(' + res.text().replace(/[\n\r\t]/g,"")+ ')');
				var el  = $Element("hbeanLeverageLayer");
				if (oResult.isPunished) {
					el.html("");
				} else {
					var decodedHtml = decodeURIComponent(oResult.html.replace(/^\s\s*/, '').replace(/\s\s*$/, ''));
					if (decodedHtml.match(/^<div.*/g)) {
						el.html(decodedHtml);
					}
				}
				el.show();
			}
		}).request({'reqparam':hbeanLeverageParam});
	},

	close : function() {
		$Element("hbeanLeverageLayer").hide();
	}
});
var happyBeanLayer = new HappyBeanLayer();

var blogId = 'kohaku3533';

var oSocialAppBox = new SocialAppAppBoxController();
</script>
