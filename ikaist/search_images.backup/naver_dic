//페이지로드, 리사이즈시 해상도 값 구해서 height 동적으로 할당
window.onresize = resize;
window.onload = resize;

function resize() {
	var clientHeight = document.documentElement.clientHeight;
	var wrapHeight = document.getElementById("wrap").offsetHeight;
	var gap = clientHeight - wrapHeight;
	var containerHeight = document.getElementById("container").offsetHeight;	

	if (wrapHeight < clientHeight) {
		document.getElementById("container").style.height = containerHeight + gap + "px";
	}

	var asideHeight = document.getElementById("aside").offsetHeight;
	var footerHeight = document.getElementById("footer").offsetHeight;

	if (containerHeight < asideHeight) {
		document.getElementById("container").style.height = asideHeight + "px";
	}
}

