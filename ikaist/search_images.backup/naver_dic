/* 나눔글꼴 기본 정보 */
var nanum = nanum || {};
nanum.Config = {
    core: {
        sCookieName                : "NNF",
        sCookieDomain            : "naver.com",
        nCookieExpire            : 3650,
        sWinSetupFileUrl        : "http://cdn.naver.com/naver/NanumFont/setup/NanumFontSetup_TTF_GOTHIC_totalsearch.exe",
        sMacSetupFileUrl        : "http://cdn.naver.com/naver/NanumFont/setupmac/NanumFontSetup_MacOS_TTF_GOTHIC_totalsearch.zip"
    },
   
    service: {
        bPopupCenterPosition    : true
    }
};
/* 나눔글꼴 설치 실행 */
nhn.jsDynamicLoad.include("/js/search_option_terms.js?201303204",function(){
    var bIPhone = (navigator.userAgent.indexOf("iPhone") > -1 ) ? true : false;
    if (bIPhone){
        nanum.Service.hideNanumArea();
        return;
    }
   	
    if (nanum.Core.isSupportedOS()){
        var bInitNanum = nanum.Core.isInitNanum();
        var bNanumInstalled = nanum.Core.isInstalled();
       
        nanum.Service.init(bInitNanum);
       
        if (bInitNanum && !bNanumInstalled) {
            nanum.Service.showNanumError();
        }   
 
        if (!nanum.Core.isSupportedClearType()) {
            if (bInitNanum) {
                nanum.Service.showClearTypeArea();
            }
        }
    } else {
        nanum.Service.hideNanumArea();
    }
});
</script>
