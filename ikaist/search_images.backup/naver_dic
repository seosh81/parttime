<script type="text/javascript">
function toString(object) {
	var string = '';

	for (key in object) {
		if (key == 'innerHTML' || key == 'textContent' || key == 'innerText' || key == 'outerText' || key == 'outerHTML') {
			continue;
		}
		string += key + ': ' + object[key] + "\n";
	}

	return string;
}
